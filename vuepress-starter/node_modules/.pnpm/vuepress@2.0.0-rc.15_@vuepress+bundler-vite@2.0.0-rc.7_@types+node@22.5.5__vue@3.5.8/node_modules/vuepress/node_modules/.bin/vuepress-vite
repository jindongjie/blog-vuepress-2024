#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.5.5__vue@3.5.8/node_modules/vuepress/bin/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.5.5__vue@3.5.8/node_modules/vuepress/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.5.5__vue@3.5.8/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.5.5__vue@3.5.8/node_modules/vuepress/bin/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.5.5__vue@3.5.8/node_modules/vuepress/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/vuepress@2.0.0-rc.15_@vuepress+bundler-vite@2.0.0-rc.7_@types+node@22.5.5__vue@3.5.8/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress-vite.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress-vite.js" "$@"
fi
