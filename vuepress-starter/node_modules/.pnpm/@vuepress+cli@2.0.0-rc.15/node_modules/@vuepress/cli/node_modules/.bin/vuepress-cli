#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.15/node_modules/@vuepress/cli/bin/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.15/node_modules/@vuepress/cli/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.15/node_modules/@vuepress/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.15/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.15/node_modules/@vuepress/cli/bin/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.15/node_modules/@vuepress/cli/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.15/node_modules/@vuepress/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/@vuepress+cli@2.0.0-rc.15/node_modules:/home/Archf0ever/Documents/Blog/vuepress-starter/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress.js" "$@"
fi
